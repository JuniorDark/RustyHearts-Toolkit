        </DataTemplate>

        <dt:PageTemplateSelector
            x:Key="StoragePageTemplateSelector"
            Page1="{StaticResource StoragePage1}"
            Page2="{StaticResource StoragePage2}"
            Page3="{StaticResource StoragePage3}"
            Page4="{StaticResource StoragePage4}"
            Page5="{StaticResource StoragePage5}" />
        <dt:PageTemplateSelector
            x:Key="AccountStoragePageTemplateSelector"
            Page1="{StaticResource AccountStoragePage1}"
            Page2="{StaticResource AccountStoragePage2}"
            Page3="{StaticResource AccountStoragePage3}"
            Page4="{StaticResource AccountStoragePage4}"
            Page5="{StaticResource AccountStoragePage5}" />
    </Window.Resources>

    <ContentControl Template="{StaticResource box_red02}">
        <Grid Margin="5,5,5,5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="30" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>

            <ui:TabView
                x:Name="tabView"
                Grid.Row="2"
                Grid.ColumnSpan="2"
                Margin="0,0,0,0"
                HorizontalAlignment="Center"
                VerticalAlignment="Top"
                Background="Transparent"
                BorderBrush="Transparent">
                <TabItem
                    x:Name="PersonalTab"
                    Width="150"
                    Foreground="White"
                    Header="Personal Warehouse"
                    Style="{StaticResource lb_tab}"
                    Tag="Page">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <StackPanel
                            Grid.Row="0"
                            Margin="0,10"
                            Orientation="Horizontal">
                            <TextBlock Foreground="White" Text="Slots Used" />
                            <TextBox
                                Width="194"
                                Height="20"
                                Margin="10,0"
                                d:Text="180"
                                IsReadOnly="True"
                                Template="{StaticResource lb_textbox11}"
                                Text="{Binding CharacterData.StorageCount}" />
                        </StackPanel>

                        <ContentControl
                            Grid.Row="1"
                            Content="{Binding CurrentStoragePage}"
                            ContentTemplateSelector="{StaticResource StoragePageTemplateSelector}" />
                        <StackPanel
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="10,0"
                            VerticalAlignment="Top"
                            Orientation="Horizontal">
                            <Image
                                Cursor="Help"
                                Source="/Assets/icon/icon_help_tip.png"
                                Stretch="None"
                                ToolTip="{x:Static rs:Resources.AttachItemHelp}" />
                        </StackPanel>
                        <StackPanel
                            Grid.Row="2"
                            Grid.Column="1"
                            HorizontalAlignment="Center"
                            Orientation="Horizontal">
                            <StackPanel.Background>
                                <ImageBrush ImageSource="/Assets/images/window/lb_box/lb_def_box10.png" />
                            </StackPanel.Background>
                            <Button
                                Command="{Binding PreviousStoragePageCommand}"
                                Foreground="White"
                                Template="{StaticResource lb_arrow_left}" />
                            <TextBlock
                                Margin="10,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Foreground="White"
                                Text="{Binding StoragePageText}" />
                            <Button
                                Command="{Binding NextStoragePageCommand}"
                                Foreground="White"
                                Template="{StaticResource lb_arrow_right}" />
                        </StackPanel>
                        <StackPanel
                            Grid.Row="3"
                            Grid.Column="1"
                            Margin="0,10"
                            Orientation="Horizontal">
                            <TextBlock Foreground="White" Text="Gold Stored" />
                            <TextBox
                                Width="194"
                                Height="20"
                                Margin="0,0"
                                d:Text="100"
                                IsReadOnly="True"
                                Template="{StaticResource lb_textbox11}"
                                Text="{Binding CharacterData.StorageGold, StringFormat={}{0:N0}}" />
                        </StackPanel>
                        <Button
                            x:Name="btnSave"
                            Grid.Row="4"
                            Grid.ColumnSpan="3"
                            Width="150"
                            Margin="0,10"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Bottom"
                            Background="Transparent"
                            Command="{Binding SaveStorageCommand}"
                            Content="Save Storage"
                            Cursor="Hand"
                            Foreground="White"
                            IsEnabled="{Binding IsButtonEnabled}"
                            Style="{StaticResource lb_def_button02}"
                            ToolTip="{x:Static rs:Resources.MailSendHelp}" />
                    </Grid>
                </TabItem>

                <TabItem
                    x:Name="AccountTab"
                    Width="150"
                    Foreground="White"
                    Header="Account Warehouse"
                    Style="{StaticResource lb_tab}"
                    Tag="Page">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <StackPanel
                            Grid.Row="0"
                            Margin="0,10"
                            Orientation="Horizontal">
                            <TextBlock Foreground="White" Text="Account Slots Used" />
                            <TextBox
                                Width="194"
                                Height="20"
                                Margin="10,0"
                                d:Text="180"
                                IsReadOnly="True"
                                Template="{StaticResource lb_textbox11}"
                                Text="{Binding AccountStorageCount}" />
                        </StackPanel>

                        <ContentControl
                            Grid.Row="1"
                            Content="{Binding CurrentAccountStoragePage}"
                            ContentTemplateSelector="{StaticResource AccountStoragePageTemplateSelector}" />
                        <StackPanel
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="10,0"
                            VerticalAlignment="Top"
                            Orientation="Horizontal">
                            <Image
                                Cursor="Help"
                                Source="/Assets/icon/icon_help_tip.png"
                                Stretch="None"
                                ToolTip="{x:Static rs:Resources.AttachItemHelp}" />
                        </StackPanel>
                        <StackPanel
                            Grid.Row="2"
                            Grid.Column="1"
                            HorizontalAlignment="Center"
                            Orientation="Horizontal">
                            <StackPanel.Background>
                                <ImageBrush ImageSource="/Assets/images/window/lb_box/lb_def_box10.png" />
                            </StackPanel.Background>
                            <Button
                                Command="{Binding PreviousAccountStoragePageCommand}"
                                Foreground="White"
                                Template="{StaticResource lb_arrow_left}" />
                            <TextBlock
                                Margin="10,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Foreground="White"
                                Text="{Binding AccountStoragePageText}" />
                            <Button
                                Command="{Binding NextAccountStoragePageCommand}"
                                Foreground="White"
                                Template="{StaticResource lb_arrow_right}" />
                        </StackPanel>
                        <StackPanel
                            Grid.Row="3"
                            Grid.Column="1"
                            Margin="0,10"
                            Orientation="Horizontal">
                            <TextBlock Foreground="White" Text="Account Gold Stored" />
                            <TextBox
                                Width="194"
                                Height="20"
                                Margin="0,0"
                                d:Text="100"
                                IsReadOnly="True"
                                Template="{StaticResource lb_textbox11}"
                                Text="{Binding AccountStorageGold, StringFormat={}{0:N0}}" />
                        </StackPanel>
                        <Button
                            x:Name="btnSaveAccountStorage"
                            Grid.Row="4"
                            Grid.ColumnSpan="3"
                            Width="150"
                            Margin="0,10"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Bottom"
                            Background="Transparent"
                            Command="{Binding SaveAccountStorageCommand}"
                            Content="Save Account Storage"
                            Cursor="Hand"
                            Foreground="White"
                            IsEnabled="{Binding IsButtonEnabled}"
                            Style="{StaticResource lb_def_button02}"
                            ToolTip="{x:Static rs:Resources.MailSendHelp}" />
                    </Grid>
                </TabItem>
            </ui:TabView>
            <!--  Close Button  -->
            <Button
                x:Name="btnClose"
                Grid.Row="0"
                Grid.ColumnSpan="2"
                Margin="0,0,0,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                bh:WindowBehaviors.CloseWindowOnClick="True"
                Template="{StaticResource lb_close_button}" />
            <TextBlock
                Grid.Row="0"
                Grid.ColumnSpan="2"
                Margin="5,0,0,0"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                d:Text="Warehouse"
                Foreground="White"
                Text="{Binding Title}" />
        </Grid>
    </ContentControl>
</Window>
