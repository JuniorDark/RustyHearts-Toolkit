                <control:ItemSlot
                    x:Name="ItemAccountStorageSlotIndex178"
                    Grid.Row="5"
                    Grid.Column="4"
                    AddItemCommand="{Binding DataContext.AddAccountStorageItemCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}"
