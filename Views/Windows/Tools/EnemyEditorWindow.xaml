                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nLevel" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </ui:NumberBox>
                                </StackPanel>
                                <StackPanel
