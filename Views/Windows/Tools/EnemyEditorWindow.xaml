<Window
    x:Class="RHToolkit.Views.Windows.EnemyEditorWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:bh="clr-namespace:RHToolkit.Behaviors"
    xmlns:control="clr-namespace:RHToolkit.Views.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dt="clr-namespace:RHToolkit.Models.DataTemplates"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:RHToolkit.Utilities.Converters"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rs="clr-namespace:RHToolkit.Properties"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    xmlns:windows="clr-namespace:RHToolkit.ViewModels.Windows"
    Title="{Binding Title}"
    MinWidth="1280"
    MinHeight="720"
    MaxHeight="{Binding Source={x:Static SystemParameters.MaximizedPrimaryScreenHeight}}"
    bh:WindowBehaviors.EnableDragMove="True"
    d:DataContext="{d:DesignInstance Type=windows:EnemyEditorViewModel}"
    d:MaxHeight="720"
    d:MaxWidth="1280"
    AllowsTransparency="True"
    Background="Transparent"
    Icon="/Assets/icon/icon_pet_gorila_red01.png"
    ResizeMode="CanResize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.InputBindings>
        <KeyBinding Command="{Binding LoadFileCommand}" Gesture="Ctrl+O" />
        <KeyBinding Command="{Binding LoadFileAsCommand}" Gesture="Ctrl+Shift+O" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileCommand}" Gesture="Ctrl+S" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileAsCommand}" Gesture="Ctrl+Alt+S" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileAsMIPCommand}" Gesture="Ctrl+Shift+S" />
        <KeyBinding Command="{Binding CloseFileCommand}" Gesture="Ctrl+W" />
        <KeyBinding
            Command="{Binding OpenSearchDialogCommand}"
            CommandParameter="Find"
            Gesture="Ctrl+F" />
        <KeyBinding
            Command="{Binding OpenSearchDialogCommand}"
            CommandParameter="Replace"
            Gesture="Ctrl+H" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="1"
            Gesture="Ctrl+Shift+1" />
    </Window.InputBindings>
    <Window.Resources>
        <!--  Template for Enemy  -->
        <DataTemplate x:Key="Enemy">
            <Grid>
                <UniformGrid Columns="6">
                    <FrameworkElement x:Name="c1" />
                </UniformGrid>
                <TabControl
                    Margin="0,0,0,0"
                    Background="Transparent"
                    BorderBrush="Transparent">
                    <TabItem
                        x:Name="tab1"
                        Width="{Binding ElementName=c1, Path=ActualWidth}"
                        Foreground="White"
                        Header="Enemy Info"
                        Style="{StaticResource lb_tab}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="ID" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataContext.DataTableManager.SelectedItem[nID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="1"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Name Text ID" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataContext.DataTableManager.SelectedItem[nNameTextID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nNameTextID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Group" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataContext.DataTableManager.SelectedItem[nGroup], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nGroup" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="3"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Sub Group" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataContext.DataTableManager.SelectedItem[nSubGroup], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nSubGroup" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="wszGroupName"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItemString[wszGroupName], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszGroupName" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemStringValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="2"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Sub Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="wszSubGroupName"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItemString[wszSubGroupName], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszSubGroupName" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemStringValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="0"
                                Grid.ColumnSpan="4"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Internal Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="wszName"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[wszName], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszName" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="3"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Group Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="wszGroupName"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[wszGroupName], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszGroupName" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="3"
                                Grid.Column="2"
                                Grid.ColumnSpan="4"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Sub Group Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="wszSubGroupName"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[wszSubGroupName], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszSubGroupName" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="4"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Face Sprite" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="szFaceSprite"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[szFaceSprite], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="szFaceSprite" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="4"
                                Grid.Column="2"
                                Grid.ColumnSpan="2"
                                Margin="10,10,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <Image
                                    Width="50"
                                    Height="44"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Source="{Binding DataContext.DataTableManager.SelectedItem[szFaceSprite], RelativeSource={RelativeSource AncestorType=Window}, Converter={StaticResource IconNameToImageSourceConverter}}"
                                    Stretch="Fill">
                                    <Image.LayoutTransform>
                                        <ScaleTransform ScaleX="1" ScaleY="1" />
                                    </Image.LayoutTransform>
                                </Image>
                            </StackPanel>

                        </Grid>
                    </TabItem>
                    <TabItem
                        x:Name="tab2"
                        Width="{Binding ElementName=c1, Path=ActualWidth}"
                        Foreground="White"
                        Header="Enemy Stats"
                        Style="{StaticResource lb_tab}">
                        <ScrollViewer
                            Margin="0,0,0,0"
                            HorizontalScrollBarVisibility="Auto"
                            Style="{StaticResource lb_scrollviewer}"
                            VerticalScrollBarVisibility="Auto">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>

                                <StackPanel
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Vertical">
                                    <CheckBox
                                        Margin="0,10,0,0"
                                        Content="Solo Boss"
                                        IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nSolo_boss], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                        Template="{StaticResource lb_checkbox}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nSolo_boss" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </CheckBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Vertical">
                                    <CheckBox
                                        Margin="0,10,0,0"
                                        Content="Can Be Grabbed"
                                        IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nCaught], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                        Template="{StaticResource lb_checkbox}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nCaught" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </CheckBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="0"
                                    Grid.Column="2"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Vertical">
                                    <CheckBox
                                        Margin="0,10,0,0"
                                        Content="No Drop"
                                        IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nNoDrop], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                        Template="{StaticResource lb_checkbox}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nNoDrop" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </CheckBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="0"
                                    Grid.Column="3"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Vertical">
                                    <CheckBox
                                        Margin="0,10,0,0"
                                        Content="Respawn Invencible"
                                        IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nRespawnGod], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                        Template="{StaticResource lb_checkbox}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nRespawnGod" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </CheckBox>
                                </StackPanel>

                                <StackPanel
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Orientation="Vertical">
                                    <TextBlock Foreground="White" Text="Mode" />
                                    <ui:NumberBox
                                        MinWidth="150"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
                                        Maximum="2147483647"
                                        Minimum="0"
                                        Value="{Binding DataContext.DataTableManager.SelectedItem[nMode], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nMode" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </ui:NumberBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Orientation="Vertical">
                                    <TextBlock Foreground="White" Text="Enemy Exp" />
                                    <ui:NumberBox
                                        MinWidth="150"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
                                        Maximum="2147483647"
                                        Minimum="0"
                                        Value="{Binding DataContext.DataTableManager.SelectedItem[fEnemyExp], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="fEnemyExp" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </ui:NumberBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="1"
                                    Grid.Column="2"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Orientation="Vertical">
                                    <TextBlock Foreground="White" Text="Enemy Gold" />
                                    <ui:NumberBox
                                        MinWidth="150"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
                                        Maximum="2147483647"
                                        Minimum="0"
                                        Value="{Binding DataContext.DataTableManager.SelectedItem[fEnemyGold], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="fEnemyGold" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </ui:NumberBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="1"
                                    Grid.Column="3"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Orientation="Vertical">
                                    <TextBlock Foreground="White" Text="Gold Probability" />
                                    <ui:NumberBox
                                        MinWidth="150"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
                                        Maximum="2147483647"
                                        Minimum="0"
                                        Value="{Binding DataContext.DataTableManager.SelectedItem[fGoldprobability], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="fGoldprobability" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </ui:NumberBox>
                                </StackPanel>

                                <StackPanel
                                    Grid.Row="2"
                                    Grid.Column="0"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Orientation="Vertical">
                                    <TextBlock Foreground="White" Text="Level" />
                                    <ui:NumberBox
                                        MinWidth="150"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
                                        Maximum="2147483647"
                                        Minimum="0"
                                        Value="{Binding DataContext.DataTableManager.SelectedItem[nLevel], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nLevel" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </ui:NumberBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Orientation="Vertical">
                                    <TextBlock Foreground="White" Text="HP" />
                                    <ui:NumberBox
                                        MinWidth="150"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
                                        Maximum="2147483647"
                                        Minimum="0"
                                        Value="{Binding DataContext.DataTableManager.SelectedItem[nHP], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nHP" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </ui:NumberBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="2"
                                    Grid.Column="2"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Orientation="Vertical">
                                    <TextBlock Foreground="White" Text="MP" />
                                    <ui:NumberBox
                                        MinWidth="150"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
                                        Maximum="2147483647"
                                        Minimum="0"
                                        Value="{Binding DataContext.DataTableManager.SelectedItem[nMP], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                        <i:Interaction.Behaviors>
                                            <bh:DataRowViewBehavior Column="nMP" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                        </i:Interaction.Behaviors>
                                    </ui:NumberBox>
                                </StackPanel>
                                <StackPanel
                                    Grid.Row="2"
                                    Grid.Column="3"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Orientation="Vertical">
                                    <TextBlock Foreground="White" Text="SP" />
                                    <ui:NumberBox
                                        MinWidth="150"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
