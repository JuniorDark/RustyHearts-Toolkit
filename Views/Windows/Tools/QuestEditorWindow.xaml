<Window
    x:Class="RHToolkit.Views.Windows.QuestEditorWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:bh="clr-namespace:RHToolkit.Behaviors"
    xmlns:control="clr-namespace:RHToolkit.Views.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dt="clr-namespace:RHToolkit.Models.DataTemplates"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:RHToolkit.Utilities.Converters"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rs="clr-namespace:RHToolkit.Properties"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    xmlns:windows="clr-namespace:RHToolkit.ViewModels.Windows"
    Title="{Binding Title}"
    MinWidth="1280"
    MinHeight="720"
    MaxHeight="{Binding Source={x:Static SystemParameters.MaximizedPrimaryScreenHeight}}"
    bh:WindowBehaviors.EnableDragMove="True"
    d:DataContext="{d:DesignInstance Type=windows:QuestEditorViewModel}"
    d:MaxHeight="720"
    d:MaxWidth="1280"
    AllowsTransparency="True"
    Background="Transparent"
    Icon="/Assets/icon/icon_story.png"
    ResizeMode="CanResize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.InputBindings>
        <KeyBinding Command="{Binding LoadFileCommand}" Gesture="Ctrl+O" />
        <KeyBinding Command="{Binding LoadFileAsCommand}" Gesture="Ctrl+Shift+O" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileCommand}" Gesture="Ctrl+S" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileAsCommand}" Gesture="Ctrl+Alt+S" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileAsMIPCommand}" Gesture="Ctrl+Shift+S" />
        <KeyBinding Command="{Binding CloseFileCommand}" Gesture="Ctrl+W" />
        <KeyBinding
            Command="{Binding OpenSearchDialogCommand}"
            CommandParameter="Find"
            Gesture="Ctrl+F" />
        <KeyBinding
            Command="{Binding OpenSearchDialogCommand}"
            CommandParameter="Replace"
            Gesture="Ctrl+H" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="1"
            Gesture="Ctrl+Shift+1" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="2"
            Gesture="Ctrl+Shift+2" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="3"
            Gesture="Ctrl+Shift+3" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="4"
            Gesture="Ctrl+Shift+4" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="5"
            Gesture="Ctrl+Shift+5" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="6"
            Gesture="Ctrl+Shift+6" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="7"
            Gesture="Ctrl+Shift+7" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="8"
            Gesture="Ctrl+Shift+8" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="9"
            Gesture="Ctrl+Shift+9" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="10"
            Gesture="Ctrl+1" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="11"
            Gesture="Ctrl+2" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="12"
            Gesture="Ctrl+3" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="13"
            Gesture="Ctrl+4" />
    </Window.InputBindings>
    <Window.Resources>
        <!--  Template for PartyMission  -->
        <DataTemplate x:Key="PartyMission">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <StackPanel
                    Grid.Row="0"
                    Grid.Column="0"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="ID" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="2147483647"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>

                <StackPanel
                    Grid.Row="0"
                    Grid.Column="1"
                    Grid.ColumnSpan="2"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Memo" />
                    <TextBox
                        Width="Auto"
                        Height="30"
                        Margin="0,0"
                        d:Text="wszMemo"
                        FontSize="12"
                        Template="{StaticResource lb_textbox11}"
                        Text="{Binding DataContext.DataTableManager.SelectedItem[wszMemo], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="wszMemo" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="0"
                    Grid.Column="3"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Note" />
                    <TextBox
                        Width="Auto"
                        Height="30"
                        Margin="0,0"
                        d:Text="wszNote"
                        FontSize="12"
                        Template="{StaticResource lb_textbox11}"
                        Text="{Binding DataContext.DataTableManager.SelectedItem[wszNote], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="wszNote" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="1"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="World ID" />
                    <ComboBox
                        Height="24"
                        MinWidth="200"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Left"
                        HorizontalContentAlignment="Left"
                        VerticalContentAlignment="Center"
                        IsReadOnly="True"
                        ItemsSource="{Binding DataContext.WorldItems, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                        SelectedValue="{Binding DataContext.DataTableManager.SelectedItem[nWorld_ID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                        SelectedValuePath="ID"
                        Style="{StaticResource ComboBoxStyle}"
                        Template="{StaticResource lb_combobox}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nWorld_ID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ComboBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="2"
                    Grid.Column="0"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Mission ID" />
                    <ComboBox
                        Height="24"
                        MinWidth="200"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Left"
                        HorizontalContentAlignment="Left"
                        VerticalContentAlignment="Center"
                        IsReadOnly="True"
                        ItemsSource="{Binding DataContext.MissionItems, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                        SelectedValue="{Binding DataContext.DataTableManager.SelectedItem[nMissionID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                        SelectedValuePath="ID"
                        Style="{StaticResource ComboBoxStyle}"
                        Template="{StaticResource lb_combobox}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nMissionID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ComboBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="1"
                    Grid.Column="2"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Orientation="Vertical">
                    <CheckBox
                        Margin="0,10,0,0"
                        Content="Daily Mission"
                        IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nDayMission], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                        Template="{StaticResource lb_checkbox}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nDayMission" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                        </i:Interaction.Behaviors>
                    </CheckBox>
                </StackPanel>

                <StackPanel
                    Grid.Row="3"
                    Grid.Column="0"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Difficult Min" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="4"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nDifficult_Min], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nDifficult_Min" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="1"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Difficult Max" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="4"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nDifficult_Max], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nDifficult_Max" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="2"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Number Min" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="8"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nNumber_Min], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nNumber_Min" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="3"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Number Max" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="8"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nNumber_Max], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nNumber_Max" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>

                <StackPanel
                    Grid.Row="4"
                    Grid.Column="0"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Gold" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="2147483647"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nGold], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nGold" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="4"
                    Grid.Column="1"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="EXP" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="2147483647"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nEXP], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nEXP" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="4"
                    Grid.Column="2"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Bonus" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="2147483647"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nMileage], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nMileage" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="4"
                    Grid.Column="3"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="PlayPoint" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="2147483647"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nPlayPoint], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nPlayPoint" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>

                <ItemsControl
                    Grid.Row="5"
                    Grid.ColumnSpan="3"
                    Margin="0,20,0,0"
                    AlternationCount="{Binding DataContext.QuestItems.Count, RelativeSource={RelativeSource AncestorType=Window}}"
                    ItemsSource="{Binding DataContext.QuestItems, RelativeSource={RelativeSource AncestorType=Window}}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <StackPanel
                                Grid.Column="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Horizontal">
                                <TextBlock
                                    Margin="0,0,10,0"
                                    VerticalAlignment="Center"
                                    Background="Transparent"
                                    FontSize="12"
                                    Foreground="Gold"
                                    Style="{StaticResource ArialMS}"
                                    Text="{Binding Path=(ItemsControl.AlternationIndex), RelativeSource={RelativeSource AncestorType=ContentPresenter}, Converter={StaticResource IndexToNumberConverter}, StringFormat='Item {0}'}" />
                                <control:ItemSlot
                                    AddItemCommand="{Binding DataContext.AddQuestItemsCommand, RelativeSource={RelativeSource AncestorType=Window}}"
                                    CommandParameter="{Binding Path=(ItemsControl.AlternationIndex), RelativeSource={RelativeSource AncestorType=ContentPresenter}}"
                                    ItemDataViewModel="{Binding ItemDataViewModel}"
                                    ItemNameVisibility="Visible"
                                    RemoveItemCommand="{Binding DataContext.RemoveQuestItemsCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                            </StackPanel>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
            </Grid>
        </DataTemplate>
        <!--  Template for Mission  -->
        <DataTemplate x:Key="Mission">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <StackPanel
                    Grid.Row="0"
                    Grid.Column="0"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="ID" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="2147483647"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>

                <StackPanel
                    Grid.Row="0"
                    Grid.Column="1"
                    Grid.ColumnSpan="3"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Description" />
                    <TextBox
                        Width="Auto"
                        Height="30"
                        Margin="0,0"
                        d:Text="wszDesc"
                        FontSize="12"
                        Template="{StaticResource lb_textbox11}"
                        Text="{Binding DataContext.DataTableManager.SelectedItem[wszDesc], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="wszDesc" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>

                <StackPanel
                    Grid.Row="2"
                    Grid.Column="0"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Mission String ID" />
                    <ComboBox
                        Height="24"
                        MinWidth="200"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Left"
                        HorizontalContentAlignment="Left"
                        VerticalContentAlignment="Center"
                        IsReadOnly="True"
                        ItemsSource="{Binding DataContext.MissionItems, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                        SelectedValue="{Binding DataContext.DataTableManager.SelectedItem[nMissionStringID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                        SelectedValuePath="ID"
                        Style="{StaticResource ComboBoxStyle}"
                        Template="{StaticResource lb_combobox}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nMissionStringID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ComboBox>
                </StackPanel>

                <StackPanel
                    Grid.Row="3"
                    Grid.Column="0"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Difficult" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="5"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nDifficult], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nDifficult" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="1"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Time Limit" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="2147483647"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nTimeLimit], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nTimeLimit" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="2"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Number Min" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="8"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nNumber_Min], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nNumber_Min" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="3"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Number Max" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Maximum="8"
                        Minimum="0"
                        Value="{Binding DataContext.DataTableManager.SelectedItem[nNumber_Max], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}">
                        <i:Interaction.Behaviors>
                            <bh:DataRowViewBehavior Column="nNumber_Max" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                        </i:Interaction.Behaviors>
                    </ui:NumberBox>
                </StackPanel>

                <StackPanel
                    Grid.Row="4"
                    Grid.Column="0"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Vertical">
                    <TextBlock Foreground="White" Text="Type" />
                    <ui:NumberBox
                        MinWidth="150"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
