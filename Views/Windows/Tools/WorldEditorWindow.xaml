<Window
    x:Class="RHToolkit.Views.Windows.WorldEditorWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:bh="clr-namespace:RHToolkit.Behaviors"
    xmlns:control="clr-namespace:RHToolkit.Views.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dt="clr-namespace:RHToolkit.Models.DataTemplates"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:RHToolkit.Utilities.Converters"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rs="clr-namespace:RHToolkit.Properties"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    xmlns:windows="clr-namespace:RHToolkit.ViewModels.Windows"
    Title="{Binding Title}"
    MinWidth="1280"
    MinHeight="720"
    MaxHeight="{Binding Source={x:Static SystemParameters.MaximizedPrimaryScreenHeight}}"
    bh:WindowBehaviors.EnableDragMove="True"
    d:DataContext="{d:DesignInstance Type=windows:WorldEditorViewModel}"
    d:MaxHeight="720"
    d:MaxWidth="1280"
    AllowsTransparency="True"
    Background="Transparent"
    Icon="/Assets/icon/lb_minimap_stage_level03.png"
    ResizeMode="CanResize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.InputBindings>
        <KeyBinding Command="{Binding LoadFileCommand}" Gesture="Ctrl+O" />
        <KeyBinding Command="{Binding LoadFileAsCommand}" Gesture="Ctrl+Shift+O" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileCommand}" Gesture="Ctrl+S" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileAsCommand}" Gesture="Ctrl+Alt+S" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileAsMIPCommand}" Gesture="Ctrl+Shift+S" />
        <KeyBinding Command="{Binding CloseFileCommand}" Gesture="Ctrl+W" />
        <KeyBinding
            Command="{Binding OpenSearchDialogCommand}"
            CommandParameter="Find"
            Gesture="Ctrl+F" />
        <KeyBinding
            Command="{Binding OpenSearchDialogCommand}"
            CommandParameter="Replace"
            Gesture="Ctrl+H" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="1"
            Gesture="Ctrl+Shift+1" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="2"
            Gesture="Ctrl+Shift+2" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="3"
            Gesture="Ctrl+Shift+3" />
    </Window.InputBindings>
    <Window.Resources>
        <!--  Template for World  -->
        <DataTemplate x:Key="World">
            <Grid>
                <UniformGrid Columns="6">
                    <FrameworkElement x:Name="c1" />
                </UniformGrid>
                <TabControl
                    Margin="0,0,0,0"
                    Background="Transparent"
                    BorderBrush="Transparent">
                    <TabItem
                        x:Name="tab1"
                        Width="{Binding ElementName=c1, Path=ActualWidth}"
                        Foreground="White"
                        Header="World Info"
                        Style="{StaticResource lb_tab}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="ID" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataContext.DataTableManager.SelectedItem[nID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="1"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Dungeon Shop" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataContext.DataTableManager.SelectedItem[nDungeonShop], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nDungeonShop" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Root Village ID 01" />
                                <ComboBox
                                    Height="24"
                                    MinWidth="200"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    HorizontalContentAlignment="Left"
                                    VerticalContentAlignment="Center"
                                    IsReadOnly="True"
                                    ItemsSource="{Binding DataContext.WorldItems, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                                    SelectedValue="{Binding DataContext.DataTableManager.SelectedItem[nRootVillageID], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                                    SelectedValuePath="ID"
                                    Style="{StaticResource ComboBoxStyle}"
                                    Template="{StaticResource lb_combobox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nRootVillageID" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                                    </i:Interaction.Behaviors>
                                </ComboBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="2"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Root Village ID 02" />
                                <ComboBox
                                    Height="24"
                                    MinWidth="200"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    HorizontalContentAlignment="Left"
                                    VerticalContentAlignment="Center"
                                    IsReadOnly="True"
                                    ItemsSource="{Binding DataContext.WorldItems, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                                    SelectedValue="{Binding DataContext.DataTableManager.SelectedItem[nRootVillageID2], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                                    SelectedValuePath="ID"
                                    Style="{StaticResource ComboBoxStyle}"
                                    Template="{StaticResource lb_combobox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nRootVillageID2" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                                    </i:Interaction.Behaviors>
                                </ComboBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="0"
                                Grid.ColumnSpan="4"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="wszNameUI"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItemString[wszNameUI], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszNameUI" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemStringValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="3"
                                Grid.Column="0"
                                Grid.ColumnSpan="4"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Internal Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="szName"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[szName], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="szName" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="4"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Internal Name UI" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="wszNameUI"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[wszNameUI], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszNameUI" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="4"
                                Grid.Column="2"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Memo" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="wszMemo"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[wszMemo], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszMemo" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="5"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Play Level" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="szPlayLevel"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[szPlayLevel], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="szPlayLevel" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="5"
                                Grid.Column="2"
                                Grid.ColumnSpan="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Play Level Blood" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="szPlayLevel"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[szPlayBloodLevel], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="szPlayBloodLevel" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="6"
                                Grid.Column="0"
                                Grid.ColumnSpan="4"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Info Sprite" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0"
                                    d:Text="szInfoSprite"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataContext.DataTableManager.SelectedItem[szInfoSprite], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="szInfoSprite" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="7"
                                Grid.Column="0"
                                Grid.ColumnSpan="4"
                                Margin="10,10,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <Image
                                    Width="420"
                                    Height="156"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Source="{Binding DataContext.DataTableManager.SelectedItem[szInfoSprite], RelativeSource={RelativeSource AncestorType=Window}, Converter={StaticResource IconNameToImageSourceConverter}}"
                                    Stretch="Fill" />
                            </StackPanel>

                        </Grid>
                    </TabItem>
                    <TabItem
                        x:Name="tab2"
                        Width="{Binding ElementName=c1, Path=ActualWidth}"
                        Foreground="White"
                        Header="Condition"
                        Style="{StaticResource lb_tab}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <CheckBox
                                    Margin="0,10,0,0"
                                    Content="Skip Dungeon Info"
                                    IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nSkipDungeonInfo], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                    Template="{StaticResource lb_checkbox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nSkipDungeonInfo" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </CheckBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="1"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <CheckBox
                                    Margin="0,10,0,0"
                                    Content="Fishing"
                                    IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nFishingGroup], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                    Template="{StaticResource lb_checkbox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nFishingGroup" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </CheckBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <CheckBox
                                    Margin="0,10,0,0"
                                    Content="Raid"
                                    IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nRaid], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                    Template="{StaticResource lb_checkbox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nRaid" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </CheckBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="3"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <CheckBox
                                    Margin="0,10,0,0"
                                    Content="Has Blood Mode"
                                    IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nBloodMode], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                    Template="{StaticResource lb_checkbox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nBloodMode" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </CheckBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="0"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <CheckBox
                                    Margin="0,10,0,0"
                                    Content="Entrance"
                                    IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nEntrance], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                    Template="{StaticResource lb_checkbox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nEntrance" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </CheckBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <CheckBox
                                    Margin="0,10,0,0"
                                    Content="Free Camera"
                                    IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nFreeCamera], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                    Template="{StaticResource lb_checkbox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nFreeCamera" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </CheckBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <CheckBox
                                    Margin="0,10,0,0"
                                    Content="No Penality"
                                    IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nNoPenalty], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                    Template="{StaticResource lb_checkbox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nNoPenalty" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </CheckBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="3"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <CheckBox
                                    Margin="0,10,0,0"
                                    Content="No Fatigue Type"
                                    IsChecked="{Binding DataContext.DataTableManager.SelectedItem[nNoFatigueType], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}"
                                    Template="{StaticResource lb_checkbox}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nNoFatigueType" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </CheckBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="0"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="World Level" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataContext.DataTableManager.SelectedItem[nWorldLevel], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nWorldLevel" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="1"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Dungeon Max Time" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataContext.DataTableManager.SelectedItem[nDungeonMaxTime], Mode=TwoWay, UpdateSourceTrigger=Explicit, RelativeSource={RelativeSource AncestorType=Window}}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nDungeonMaxTime" UpdateItemValueCommand="{Binding DataContext.DataTableManager.UpdateItemValueCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="2"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Stage Play Point" />
                                <ui:NumberBox
                                    MinWidth="150"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
