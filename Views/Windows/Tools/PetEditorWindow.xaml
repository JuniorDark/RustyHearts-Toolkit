                            </StackPanel>
                            <StackPanel
                                Grid.Row="5"
                                Grid.Column="2"
                                Margin="58,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <Image
                                    Width="36"
                                    Height="36"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Source="{Binding DataTableManager.SelectedItem[szBetrayalIcon], Converter={StaticResource IconNameToImageSourceConverter}}"
                                    Stretch="Fill" />
                            </StackPanel>
                            <StackPanel
                                Grid.Row="6"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Pet Eat Icon" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    d:Text="szPetEatIcon"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItem[szPetEatIcon], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="szPetEatIcon" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="6"
                                Grid.Column="2"
                                Margin="58,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <Image
                                    Width="36"
                                    Height="36"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Source="{Binding DataTableManager.SelectedItem[szPetEatIcon], Converter={StaticResource IconNameToImageSourceConverter}}"
                                    Stretch="Fill" />
                            </StackPanel>
                            <StackPanel
                                Grid.Row="7"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Dead Icon" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    d:Text="szDeadIcon"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItem[szDeadIcon], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="szDeadIcon" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="7"
                                Grid.Column="2"
                                Margin="58,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <Image
                                    Width="36"
                                    Height="36"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Source="{Binding DataTableManager.SelectedItem[szDeadIcon], Converter={StaticResource IconNameToImageSourceConverter}}"
                                    Stretch="Fill" />
                            </StackPanel>
                            <StackPanel
                                Grid.Row="8"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Description" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    d:Text="Desc"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItem[wsz_Desc_MEMO], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wsz_Desc_MEMO" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                        </Grid>
                    </TabItem>
                    <TabItem
                        Width="{Binding ElementName=c2, Path=ActualWidth}"
                        Foreground="White"
                        Header="Pet Rules"
                        Style="{StaticResource lb_tab}">
                        <Grid HorizontalAlignment="Center">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Pet ID" />
                                <ui:NumberBox
                                    Width="Auto"
                                    MinWidth="140"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="2147483647"
                                    Minimum="0"
                                    Value="{Binding DataTableManager.SelectedItem[nID], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nID" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="1"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Pet Type" />
                                <ui:NumberBox
                                    MinWidth="140"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="100"
                                    Value="{Binding DataTableManager.SelectedItem[nPetType], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nPetType" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="3"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Pet Group" />
                                <ui:NumberBox
                                    MinWidth="140"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="999"
                                    Minimum="1"
                                    Value="{Binding DataTableManager.SelectedItem[nPetGroup], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nPetGroup" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="3"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Inventory Count" />
                                <ui:NumberBox
                                    MinWidth="140"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Maximum="24"
                                    Minimum="0"
