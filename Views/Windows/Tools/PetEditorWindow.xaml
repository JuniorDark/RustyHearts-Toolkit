                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <Image
                                    Width="60"
                                    Height="60"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Source="{Binding DataTableManager.SelectedItem[szBigIcon], Converter={StaticResource IconNameToImageSourceConverter}}"
                                    Stretch="Fill" />
                            </StackPanel>
                            <StackPanel
                                Grid.Row="5"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Betrayal Icon" />
