                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="nInvenCount" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </ui:NumberBox>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="0"
