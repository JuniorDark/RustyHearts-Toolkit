<Window
    x:Class="RHToolkit.Views.Windows.PetEditorWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:bh="clr-namespace:RHToolkit.Behaviors"
    xmlns:control="clr-namespace:RHToolkit.Views.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:RHToolkit.Utilities.Converters"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rs="clr-namespace:RHToolkit.Properties"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    xmlns:windows="clr-namespace:RHToolkit.ViewModels.Windows"
    Title="{Binding Title}"
    MinWidth="1280"
    MinHeight="720"
    MaxHeight="{Binding Source={x:Static SystemParameters.MaximizedPrimaryScreenHeight}}"
    bh:WindowBehaviors.EnableDragMove="True"
    d:DataContext="{d:DesignInstance Type=windows:PetEditorViewModel}"
    d:MaxHeight="720"
    d:MaxWidth="1280"
    AllowsTransparency="True"
    Background="Transparent"
    Icon="/Assets/icon/icon_pet_chick_inven01.png"
    ResizeMode="CanResize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.InputBindings>
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="1"
            Gesture="Ctrl+Shift+1" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="2"
            Gesture="Ctrl+Shift+2" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="3"
            Gesture="Ctrl+Shift+3" />
        <KeyBinding
            Command="{Binding LoadFileCommand}"
            CommandParameter="4"
            Gesture="Ctrl+Shift+4" />
        <KeyBinding Command="{Binding LoadFileCommand}" Gesture="Ctrl+O" />
        <KeyBinding Command="{Binding LoadFileAsCommand}" Gesture="Ctrl+Shift+O" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileCommand}" Gesture="Ctrl+S" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileAsCommand}" Gesture="Ctrl+Alt+S" />
        <KeyBinding Command="{Binding DataTableManager.SaveFileAsMIPCommand}" Gesture="Ctrl+Shift+S" />
        <KeyBinding Command="{Binding CloseFileCommand}" Gesture="Ctrl+W" />
        <KeyBinding
            Command="{Binding OpenSearchDialogCommand}"
            CommandParameter="Find"
            Gesture="Ctrl+F" />
        <KeyBinding
            Command="{Binding OpenSearchDialogCommand}"
            CommandParameter="Replace"
            Gesture="Ctrl+H" />
    </Window.InputBindings>
    <ContentControl Template="{StaticResource box_violet02}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="50" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="40" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <ui:TitleBar
                x:Name="TitleBar"
                Title="{Binding Title}"
                Grid.ColumnSpan="3"
                Height="30"
                Margin="30,0,0,0"
                VerticalAlignment="Center"
                d:Title="Pet Editor"
                ButtonsForeground="White"
                FontSize="15"
                Foreground="White">
                <ui:TitleBar.Icon>
                    <ui:ImageIcon
                        Width="16"
                        Height="16"
                        Margin="0,-1,0,-3"
                        Source="/Assets/icon/icon_pet_chick_inven01.png" />
                </ui:TitleBar.Icon>
            </ui:TitleBar>

            <control:EditorMenu
                Grid.Row="1"
                Grid.Column="0"
                Grid.ColumnSpan="2" />

            <!--  Controls  -->
            <Grid
                Grid.Row="1"
                Grid.RowSpan="3"
                Grid.Column="2"
                Grid.ColumnSpan="2"
                Visibility="{Binding IsVisible}">
                <UniformGrid Columns="4">
                    <FrameworkElement x:Name="c2" />
                </UniformGrid>
                <TabControl Margin="0,0,0,0">
                    <TabItem
                        Width="{Binding ElementName=c2, Path=ActualWidth}"
                        Foreground="White"
                        Header="Pet Data"
                        Style="{StaticResource lb_tab}">
                        <Grid HorizontalAlignment="Center">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="371*" />
                                <ColumnDefinition Width="277*" />
                                <ColumnDefinition Width="94*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Pet Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    d:Text="Name"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItemString[wszName], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszName" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemStringValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.ColumnSpan="3"
                                Margin="370,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Pet Name (Internal)" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="10,0,0,0"
                                    d:Text="Name"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItem[wszName], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszName" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="2"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Group Name" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    d:Text="Group Name"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItemString[wszGroupName], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszGroupName" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemStringValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="2"
                                Grid.ColumnSpan="3"
                                Margin="370,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Group Name (Internal)" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="10,0,0,0"
                                    d:Text="Group Name"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItem[wszGroupName], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="wszGroupName" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="3"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Icon" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    d:Text="szIcon"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItem[szIcon], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                                    <i:Interaction.Behaviors>
                                        <bh:DataRowViewBehavior Column="szIcon" UpdateItemValueCommand="{Binding DataTableManager.UpdateItemValueCommand}" />
                                    </i:Interaction.Behaviors>
                                </TextBox>
                            </StackPanel>
                            <StackPanel
                                Grid.Row="3"
                                Grid.Column="2"
                                Margin="58,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <Image
                                    Width="36"
                                    Height="36"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Source="{Binding DataTableManager.SelectedItem[szIcon], Converter={StaticResource IconNameToImageSourceConverter}}"
                                    Stretch="Fill" />
                            </StackPanel>
                            <StackPanel
                                Grid.Row="4"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="Big Icon" />
                                <TextBox
                                    Width="Auto"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    d:Text="szBigIcon"
                                    FontSize="12"
                                    Template="{StaticResource lb_textbox11}"
                                    Text="{Binding DataTableManager.SelectedItem[szBigIcon], Mode=TwoWay, UpdateSourceTrigger=Explicit}">
