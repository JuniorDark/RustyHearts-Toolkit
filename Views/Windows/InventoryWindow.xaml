            <!--  Close Button  -->
            <Button
                x:Name="btnClose"
                Grid.Row="0"
                Grid.ColumnSpan="2"
                Margin="0,0,0,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                bh:WindowBehaviors.CloseWindowOnClick="True"
                Template="{StaticResource lb_close_button}" />
            <TextBlock
                Grid.Row="0"
                Grid.ColumnSpan="2"
                Margin="5,0,0,0"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                d:Text="Character Inventory"
                Foreground="White"
                Text="{Binding Title}" />
            <StackPanel
                Grid.Row="2"
                Grid.Column="0"
                Orientation="Horizontal">
                <Image
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Cursor="Help"
                    Source="/Assets/icon/icon_help_tip.png"
                    Stretch="None"
                    ToolTip="{x:Static rs:Resources.AttachItemHelp}" />
            </StackPanel>
            <StackPanel
                Grid.Row="2"
                Grid.Column="1"
                HorizontalAlignment="Right"
                Orientation="Horizontal"
                Visibility="{Binding SelectedItem, ElementName=tabView, Converter={StaticResource TabToVisibilityConverter}}">
                <StackPanel.Background>
                    <ImageBrush ImageSource="/Assets/images/window/lb_box/lb_def_box10.png" Stretch="UniformToFill" />
                </StackPanel.Background>
                <Button
                    Command="{Binding PreviousPageCommand}"
                    Foreground="White"
                    Template="{StaticResource lb_arrow_left}" />
                <TextBlock
                    Margin="10,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Foreground="White"
                    Text="{Binding PageText}" />
                <Button
                    Command="{Binding NextPageCommand}"
                    Foreground="White"
                    Template="{StaticResource lb_arrow_right}" />
            </StackPanel>
            <StackPanel
                Grid.Row="3"
                Grid.Column="0"
                Orientation="Vertical">
                <TextBlock
                    Grid.Row="3"
                    Grid.Column="0"
                    Margin="5,0,0,5"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Foreground="White"
                    Text="Gold In Possession" />
                <TextBlock
                    Grid.Row="3"
                    Grid.Column="0"
                    Margin="5,0,0,5"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Foreground="White"
                    Text="Current Zen" />
                <TextBlock
                    Grid.Row="3"
                    Grid.Column="0"
                    Margin="5,0,0,5"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Foreground="White"
                    Text="Bonus Cash" />
                <TextBlock
                    Grid.Row="3"
                    Grid.Column="0"
                    Margin="5,0,0,5"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Foreground="White"
                    Text="Ressurection Scroll" />
            </StackPanel>
            <StackPanel
                Grid.Row="3"
                Grid.Column="1"
                Orientation="Vertical">
                <TextBox
                    Grid.Row="3"
                    Grid.Column="1"
                    Width="194"
                    Height="20"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    d:Text="100"
                    IsReadOnly="True"
                    Template="{StaticResource lb_textbox11}"
                    Text="{Binding CharacterData.Gold, StringFormat={}{0:N0}}" />
                <TextBox
                    Grid.Row="3"
                    Grid.Column="1"
                    Width="194"
                    Height="20"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    d:Text="0"
                    IsReadOnly="True"
                    Template="{StaticResource lb_textbox11}"
                    Text="{Binding CashValue, StringFormat={}{0:N0}}" />
                <TextBox
                    Grid.Row="3"
                    Grid.Column="1"
                    Width="194"
                    Height="20"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    d:Text="0"
                    IsReadOnly="True"
                    Template="{StaticResource lb_textbox11}"
                    Text="{Binding BonusCashValue, StringFormat={}{0:N0}}" />
                <TextBox
                    Grid.Row="3"
                    Grid.Column="1"
                    Width="194"
                    Height="20"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    d:Text="0"
                    IsReadOnly="True"
                    Template="{StaticResource lb_textbox11}"
                    Text="{Binding CharacterData.Hearts}" />
            </StackPanel>
            <Button
                x:Name="btnSave"
                Grid.Row="4"
                Grid.ColumnSpan="3"
                Width="150"
                Margin="0,10"
                HorizontalAlignment="Center"
                VerticalAlignment="Bottom"
                Background="Transparent"
                Command="{Binding SaveInventoryCommand}"
                Content="Save"
                Cursor="Hand"
                Foreground="White"
                IsEnabled="{Binding IsButtonEnabled}"
                Style="{StaticResource lb_def_button02}"
                ToolTip="{x:Static rs:Resources.MailSendHelp}" />

        </Grid>
    </ContentControl>
</Window>
